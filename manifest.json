{
  "manifest_version": 3,
  "name": "P2P manager",
  "description": "A chrome extension for managing p2p orders",
  "action": {
    "default_icon": "p2p.png"
  },
  "icons": {
    "128": "p2p.png"
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.binance.com/*",
        "https://p2p.binance.com/*"
      ],
      "js": [
        "bundle.js"
      ]
    }
  ]
}
