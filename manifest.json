{
  "manifest_version": 3,
  "name": "P2P manager",
  "description": "A chrome extension for managing p2p orders",
  "action": {
    "default_icon": "p2p.png"
  },
  "icons": {
    "128": "p2p.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "<all_urls>"
      ],
      "js": [
        "bundle.js"
      ]
    }
  ]
}
